//Q179. Largest Number 
class Solution {
public:
   static bool mycomp(string a , string b)
   {
    //string t1=a+b;
    //string t2=b+a;
    return a+b>b+a;
   }
    string largestNumber(vector<int>& nums) {
    vector<string>snums;
    for(auto s:nums)
    {
        // copy values in nums int vector to string vector 
        snums.push_back(to_string(s));
    } 
    sort(snums.begin(),snums.end(),mycomp);   
    // arrange string according to custom arragement
    string ans="";
    //handling test case 0,0
    if(snums[0]=="0")
    {
        return "0";
    }
    for(auto n:snums)
    {
        ans=ans+n;
    }
    return ans;
    }
};
